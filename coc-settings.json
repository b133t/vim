{
  "coc.preferences.diagnostic.signOffset": 3000,
  "coc.preferences.diagnostic.errorSign": "▶",
  "coc.preferences.diagnostic.warningSign": "▷",
  "coc.preferences.diagnostic.infoSign": "ℹ",
  "coc.preferences.diagnostic.hintSign": "•",

  "coc.preferences.hoverTarget": "float",

  "coc.preferences.useQuickfixForLocations": true,

  "coc.preferences.triggerAfterInsertEnter": true,
  "coc.preferences.hyphenAsKeyword": true,

  "coc.preferences.timeout": 2000,
  "coc.preferences.acceptSuggestionOnCommitCharacter": true,

  "coc.preferences.snippetIndicator": " ★",

  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": [
  ],

  "python.jediEnabled": false,
  "python.formatting.provider": "yapf",

  "tsserver.typingsCacheLocation": "~/.cache/tsserver",
  "tsserver.log": "normal",
  "tsserver.orgnizeImportOnSave": true,

  "languageserver": {
    "kotlin": {
      "command": "kotlin-language-server",
      "args": [],
      "filetypes": ["kotlin", "kt"],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
